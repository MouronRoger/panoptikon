[MASTER]
ignore=CVS
persistent=yes
load-plugins=

[FORMAT]
max-line-length=120
max-module-lines=500
indent-after-paren=4

[MESSAGES CONTROL]
disable=C0111,R0903,C0103,R0913,C0330,C0326,R0914,R0912,R0915,R0902,R0904,R0911,R0801,W0511

[BASIC]
good-names=i,j,k,ex,Run,_,id,fp,fn

[REPORTS]
output-format=text
files-output=no
reports=no
evaluation=10.0 - ((float(5 * error + warning + refactor + convention) / statement) * 10)

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no

[VARIABLES]
init-import=no
dummy-variables-rgx=_$|dummy
additional-builtins=

[TYPECHECK]
ignore-mixin-members=yes
ignored-classes=SQLObject
unsafe-load-any-extension=yes

[MISCELLANEOUS]
notes=FIXME,XXX,TODO

[IMPORTS]
deprecated-modules=regsub,TERMIOS,Bastion,rexec
import-graph=
ext-import-graph=
int-import-graph=